//синонимы
sc_node_not_relation -> dynamic_web_page;;
dynamic_web_page => nrel_main_idtf :[веб-страница] (* <- lang_ru;; *);;
dynamic_web_page => nrel_main_idtf :[dynamic web page](* <- lang_en;; *);;

//определение 

dynamic_web_page <- rrel_key_sc_element: 
        ...
	(*
	=> nrel_main_idtf: [Опр.(веб-страница)]  (* <- lang_ru;; *);;
	<- sc_explanation;; 
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Динамическая веб-страница — страница, на которой присутствуют javascript сценарии]
			(* <- lang_ru;; *);;
		*);; 
	*);;


//утверждения

