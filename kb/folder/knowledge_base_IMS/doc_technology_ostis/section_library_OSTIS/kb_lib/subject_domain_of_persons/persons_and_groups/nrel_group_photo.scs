nrel_group_photo

=> nrel_main_idtf:
	[коллективная фотография*] (* <- lang_ru;; *);
	[group photo*] (* <- lang_en;; *);

<- binary_relation;

=> nrel_first_domain:
	person;
	
<- rrel_key_sc_element:
	...
	(*
		<- sc_explanation;;
		<= nrel_sc_text_translation:
		...
			(*
				-> rrel_example:
					"file://content/explanation_for_nrel_group_photo.html"
					(*<- lang_ru;;*);;
			*);;
	*);

<- rrel_key_sc_element:
	...
	(*
		<- typical_sc_neighborhood;;
		<= nrel_sc_text_translation:
		...
		
			(*
				<= nrel_sctrict_inclusion: 
					sc.g_text;;
				-> rrel_example:
					"file://content/nrel_group_photo.png";;
			*);;
	*);

<- rrel_key_sc_element:
	...
	(*
		<- image;;
		<= nrel_sc_text_translation:
		...
		
			(*
				-> rrel_example:
					"file://content/nrel_group_photo.jpg";;
			*);;
	*);;
